# إعدادات العامة
PORT=5000
NODE_ENV=production
SESSION_SECRET=your_session_secret_here

# إعدادات قاعدة بيانات PostgreSQL
DATABASE_URL=postgres://username:password@host:port/database
PGUSER=username
PGPASSWORD=password
PGDATABASE=database
PGHOST=host
PGPORT=5432

# إعدادات قاعدة بيانات MongoDB (اختياري للتحليلات)
MONGODB_URI=mongodb+srv://username:password@host/database

# إعدادات API للذكاء الاصطناعي
AI_API_KEY=your_ai_api_key_here

# إعدادات أخرى
SAFE_MODE=false